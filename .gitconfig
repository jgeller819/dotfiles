[user]
	name = Josh Geller
	email = jgeller819@gmail.com
[alias]
	co = checkout
	unstage = reset HEAD --
	last = log -l HEAD
        ci = commit
        st = status
        br = branch
        stat = status
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        type = cat-file -t
        dump = cat-file -p
        branches = branch -a
        remotes = remote -v
[color]
        ui = auto
[color "branch"]
       current = yellow reverse
       local = yellow
       remote = green
[color "diff"]
       meta = yellow bold
       frag = magenta bold
       old = red bold
       new = green bold
[color "status"]
       added = yellow
       changed = green
       untracked = cyan
[merge]
        log = true
[core]
	editor = emacs -nw
	autocrlf = input
	safecrlf = true
[push]
	default = simple
